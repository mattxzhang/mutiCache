<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dyy.yonxin.library2.cacheforandroid">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".CacheForAndorid"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShareActivity" />
        <activity android:name=".PropertiesActivity" />
        <activity android:name=".SeriObjActivity" />
        <activity android:name=".DBActivity" />
        <activity android:name=".DBStateActivity" />
        <activity android:name=".DBNoKeyActivity" />
        <activity android:name=".CompositeCacheActivity"></activity>
    </application>

</manifest>